prototype(Neos.MarketPlace:PackageLastActivity) < prototype(Neos.Fusion:Component) {
    lastActivity = ${q(node).property('lastActivity')}

    renderer = afx`
        <i class="fas fa-clock"></i> {MarketPlace.Date.formatRelative(props.lastActivity)}
    `

    @if.hasLastActivity = ${this.lastActivity}
}
