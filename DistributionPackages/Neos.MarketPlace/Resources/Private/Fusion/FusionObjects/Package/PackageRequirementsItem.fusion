prototype(Neos.MarketPlace:PackageRequirementsItem) < prototype(Neos.Fusion:Component) {
    package = null
    version = ''

    renderer = afx`
        <li class="requirements-item">
            <a title={props.package + ' @ ' + props.version}>
                <Neos.MarketPlace:PackageUri packageKey={props.package} @path="attributes.href" />
                {props.package}
            </a>:{props.version}
        </li>
    `
}
