/**
* Basic page inside the event package
*/

prototype(Neos.NeosIo.Event:Page) < prototype(Neos.NeosIo.Event:DefaultPage) {
	body {
		content {
			main = Neos.NeosIo.Event:Page
		}
	}
}

prototype(Neos.NeosIo.Event:Page) < prototype(Neos.Neos:Content) {
	main = Neos.Neos:ContentCollection {
		@cache.mode = 'cached'
		nodePath = 'main'
	}
}
