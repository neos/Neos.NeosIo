prototype(Neos.NodeTypes:Page) < prototype(Neos.NeosIo:DefaultPage) {
    body {
        templatePath = 'resource://Neos.NeosIo/Private/Templates/Page/Default.html'
        content {
            // The default content section
            main = Neos.Neos:PrimaryContent {
                nodePath = 'main'
            }
        }
    }
}
