{
    "cache": true,
    "devFile": "modernizr.js",
    "dest": "Resources/Public/JavaScript/Modernizr.min.js",
    "options": [
        "setClasses",
        "html5printshiv",
        "testProp",
        "fnBind"
    ],
    "uglify": true,
    "crawl": true,
    "useBuffers": false,
    "files": {
        "src": [
            "Resources/Private/**/*.{js,scss}"
        ]
    }
}
