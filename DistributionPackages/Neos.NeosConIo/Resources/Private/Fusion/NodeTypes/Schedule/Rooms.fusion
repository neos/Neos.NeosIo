prototype(Neos.NeosConIo:Rooms) < prototype(Neos.Fusion:Component) {
    rooms = ${q(event).find('[instanceof Neos.NeosConIo:Room]').get()}

    renderer = afx`
        <Neos.Fusion:Collection collection={props.rooms} itemName="room" @children="itemRenderer">
            <button class="rooms__room" data-room-id={room.identifier}>
                {q(room).property('name')}
            </button>
        </Neos.Fusion:Collection>
    `
}
