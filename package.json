    "watch": "yarn watch:js & yarn watch:css",
    "watch:js": "node Build/Carbon.Pipeline/esbuild.mjs --watch",
    "watch:css": "node Build/Carbon.Pipeline/postcss.mjs --watch",
    "pipeline": "yarn install;yarn build"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
